                    </div>
                </div>
            </div>

            <!-- end page title end breadcrumb -->



            <div class="page-content-wrapper">

                <div class="container-fluid">

                    <div class="row">

                        <div class="col-12">

                            <div class="card">

                                <div class="card-body">



                                    <h4 class="header-title">Pembayaran</h4>

                                    <p class="card-title-desc">
                                    Disini adalah detail pembayaran yang dilakukan oleh orang-orang
                                    </p>
                                    <table border="1" id="datatable" class="table table-bordered dt-responsive nowrap"
                                        style="border-collapse: collapse; border-spacing: 0; width: 100%;">
                                        <thead>
                                            <tr>
                                                <th>Cek</th>
                                                <th>Nominal</th>
                                                <th>Kurir</th>
                                                <th>No Rek</th>
                                                <th>First Name</th>
                                                <th>Last Name</th>
                                                <th>Email</th>
                                                <th>Validasi</th>
                                                <th>Alamat</th>
                                                <th>Provinsi</th>
                                                <th>Kabupaten/Kota</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                        <% pembayaran.forEach(function(item) { %>
                                        <tr>
                                            <% 
                                            var url=`https://api-dieng.primakom.co.id/images/pembayaran_${item.getDataValue('userid')}.png`;
                                            %>
                                            <td><a href="<%=url%>" class="btn btn-block btn-warning"><%=item.getDataValue('userid')%></a></td>
                                            <td><%= item.getDataValue('nominal') %></td>
                                            <td><%= item.getDataValue('kurir') %></td>
                                            <td><%= item.getDataValue('no_rek') %></td>
                                            <% user.forEach(function(alam) { %>
                                            <% if (item.getDataValue('userid') == alam.getDataValue('id')) { %>
                                            <td><%= alam.getDataValue('first_name') %></td>
                                            <td><%= alam.getDataValue('last_name') %></td>
                                            <td><%= alam.getDataValue('email') %></td>
                                            <td>
                                                <% if (null == alam.getDataValue('valid')) { %>
                                                <% var urln =
                                                "https://api-dieng.primakom.co.id/users/validasi/"+alam.getDataValue('id'); %>
                                                <a href=<%=urln%>>Validasi</a> | 
                                                    Invalid
                                                <% } else { %>
                                                    Valid
                                                <% } %>
                                            </td>
                                            <% } %>
                                            <% }); %>
                                            <% profile.forEach(function(elem) { %>
                                            <% if (item.getDataValue('userid') == elem.getDataValue('user_id')) { %>
                                            <td><%= elem.getDataValue('alamat') %></td>
                                            <td><%= elem.getDataValue('provinsi') %></td>
                                            <td><%= elem.getDataValue('kabupaten') %></td>
                                            <% } %>
                                            <% }); %>
                                        </tr>
                                        <% }); %>

                                        </tbody>
                                    </table>
                                </div>

                            </div>

                        </div> <!-- end col -->

                    </div> <!-- end row -->



                </div>

                <!-- end container-fluid -->

            </div> 

